meta {
  name: Register
  type: http
  seq: 1
}

post {
  url: http://localhost:8000/api/register
  body: json
  auth: inherit
}

body:json {
  {
    "email": "{{email}}",
    "password": "{{password}}",
    "firstname": "{{firstname}}",
    "lastname": "{{lastname}}",
    "phone": "{{phone}}",
    "city": "{{city}}"
  }
}

settings {
  encodeUrl: true
}
